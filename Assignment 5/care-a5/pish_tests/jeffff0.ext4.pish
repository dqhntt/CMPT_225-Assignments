c = 30;

while a > 0 {
    if a * 2 > c {
        print 1;
    } 
    elif c > b * 2 {
        print 0;
    }

    if a + b == c {
        if c - a == b {
            if a + a == b {
                if a > b {
                    print 0;
                } 
                elif b > c {
                    print 0;
                }
                elif c < a {
                    print 0;
                }
                elif b > c {
